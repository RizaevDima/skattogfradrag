<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.skattfradrag" version="1.2.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Skatt Fradrag</name>
    <description>Skatt Fradrag accounting costs app. Take a photo with your mobile of your check or just choose from your photo library and upload to the app for accounting.</description>
    <author email="post@risingbear.no" href="http://phonegap.com">oleg</author>
    <content src="index.html" />
    <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryUsageDescription">
        <string>Our application uses photo library so you can choose a picture of the check and send it to us for accounting.</string>
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSCameraUsageDescription">
        <string>Our application uses a camera so you can take a picture of the check and send it to us for accounting.</string>
    </edit-config>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-maxSdkVersion" value="29" />
    <preference name="android-minSdkVersion" value="25" />
    <preference name="android-targetSdkVersion" value="29" />
    <preference name="phonegap-version" value="cli-7.1.0" />
</widget>
